prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
prefix counterparty: <http://localhost:9000/cs/client/counterparty/>
prefix client: <http://localhost:9000/cs/client>

client:Counterparty a rdfs:Class;
    rdfs:description "counterparty details";
    rdfs:label "Counterparty";
    rdfs:Property counterparty:counterpartyId;
    rdfs:Property counterparty:firstName;
    rdfs:Property counterparty:lastName;
    rdfs:Property counterparty:dateOfBirth;
    rdfs:Property counterparty:email;
    rdfs:Property counterparty:phone.
